{
  "name": "mobx-decorators",
  "version": "5.0.3",
  "description": "Several helper MobX decorators (setter, observe, save, ...)",
  "repository": "farwayer/mobx-decorators",
  "author": "farwayer <farwayer@gmail.com>",
  "license": "WTFPL",
  "keywords": [
    "mobx",
    "decorator",
    "setter",
    "set",
    "toggle",
    "intercept",
    "observe",
    "save"
  ],
  "main": "lib",
  "jsnext:main": "esm",
  "module": "esm",
  "react-native": "esm",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build-lib && yarn build-esm",
    "build-lib": "babel src --out-dir lib",
    "build-esm": "ESM=1 babel src --out-dir esm",
    "clean": "rm -rf lib esm",
    "test": "mocha",
    "cover": "istanbul cover _mocha --report lcovonly -- --reporter spec && cat coverage/lcov.info | coveralls && rm -rf coverage",
    "prepack": "yarn build && yarn test"
  },
  "peerDependencies": {
    "mobx": "^4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@types/should": "^13.0.0",
    "coveralls": "^3.0.3",
    "istanbul": "1.1.0-alpha.1",
    "mobx": "^5.9.4",
    "mocha": "^6.1.4",
    "should": "^13.2.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "decorating": "^1.0.3",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0"
  }
}
